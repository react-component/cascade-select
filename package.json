{
  "name": "rc-cascade-select",
  "version": "0.1.0",
  "description": "cascade-select ui component for react",
  "keywords": [
    "react",
    "react-component",
    "react-cascade-select",
    "cascade-select"
  ],
  "homepage": "http://github.com/react-component/cascade-select",
  "author": "yiminghe@gmail.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:react-component/cascade-select.git"
  },
  "bugs": {
    "url": "http://github.com/react-component/cascade-select/issues"
  },
  "licenses": "MIT",
  "spm": {
    "dependencies": {
      "react": "*"
    }
  },
  "config": {
    "port": 8000
  },
  "scripts": {
    "example": "webpack -d && rc-tools run jsx2html",
    "less": "rc-tools run less",
    "history": "rc-tools run history",
    "start": "node-dev --harmony node_modules/.bin/rc-server",
    "publish": "spm publish && rc-tools run tag",
    "lint": "rc-tools run lint",
    "test": "",
    "saucelabs": "rc-tools run saucelabs",
    "browser-test": "rc-tools run browser-test",
    "browser-test-cover": "rc-tools run browser-test-cover"
  },
  "devDependencies": {
    "css-loader": "^0.9.1",
    "expect.js": "~0.3.1",
    "file-loader": "^0.8.1",
    "jsx-loader": "^0.12.2",
    "node-dev": "2.x",
    "precommit-hook": "^1.0.7",
    "rc-server": "~2.1.2",
    "rc-tools": "~1.2.8",
    "react": "~0.13.0",
    "style-loader": "^0.8.3",
    "url-loader": "^0.5.5",
    "webpack": "~1.5.3"
